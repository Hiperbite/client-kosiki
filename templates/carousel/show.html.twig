{% extends 'base.html.twig' %}

{% block title %}Carousel{% endblock %}

{% block body %}
    <h1>Carousel</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ carousel.id }}</td>
            </tr>
            <tr>
                <th>Tittle</th>
                <td>{{ carousel.tittle }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ carousel.description }}</td>
            </tr>
            <tr>
                <th>Is_active</th>
                <td>{{ carousel.isActive ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>ImageFilename</th>
                <td>{{ carousel.imageFilename }}</td>
            </tr>
            <tr>
                <th>Order_no</th>
                <td>{{ carousel.orderNo }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_carousel_index') }}">back to list</a>

    <a href="{{ path('app_carousel_edit', {'id': carousel.id}) }}">edit</a>

    {{ include('carousel/_delete_form.html.twig') }}
{% endblock %}
